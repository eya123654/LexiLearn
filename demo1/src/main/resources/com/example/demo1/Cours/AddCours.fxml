<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml">
    <TableView fx:id="courseTable">
        <columns>
            <TableColumn text="ID" fx:id="idColumn"/>
            <TableColumn text="Name" fx:id="nameColumn"/>
            <TableColumn text="Description" fx:id="descriptionColumn"/>
            <TableColumn text="Progress" fx:id="progressColumn"/>
            <TableColumn text="Image" fx:id="imageColumn"/>
            <TableColumn text="Price" fx:id="priceColumn"/>
        </columns>
    </TableView>
    <HBox>
        <Button text="Add" fx:id="addButton"/>
        <Button text="Edit" fx:id="editButton"/>
        <Button text="Delete" fx:id="deleteButton"/>
    </HBox>
</AnchorPane>
